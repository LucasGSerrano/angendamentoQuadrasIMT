<body>
	<h4>Sistema de Agendamento de Quadras</h4>
	<%= form_for( :login, url: login_path) do |f| %>	
		<div class="pag_login">
			<label id="nome_usuario" <%= f.label :nome %>></label>
	     		<%= f.text_field :nome %><br/><br/>
	       	<label id="senha">Senha:</label>
			<%= f.password_field :senha %><br/><br/>
			<%= submit_tag("Log In") %>
		</div>
	<% end %>
</body>